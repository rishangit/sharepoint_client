{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "baseUrl": ".",
    // "paths": {
    //   "@primitives": ["./src/components/primitives"],
    //   "@sys/action":["./src/system/action"],
    //   "@sys/const":["./src/system/constant/constant"],
    //   "@httpCall":["./src/system/httpCall"],
    // }
  },
  "include": [
    "src",
    "**/*.ts",
    "**/*.tsx"
, "config-overrides.js"  ]
}
